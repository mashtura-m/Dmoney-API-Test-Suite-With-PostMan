				{
					"name": "Admin Login - Valid",
					"event": [
						{
							"listen": "test",
							"script": {
								"id": "dc421506-41a3-4057-a2e3-953ead2d9ddc",
								"exec": [
									"// Basic status check",
									"pm.test(\"Status code is 200\", function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"// Token existence check",
									"pm.test(\"Response contains token\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData).to.have.property(\"token\");",
									"});",
									"",
									"// Save admin token to BOTH environment and collection variables",
									"if (pm.response.code === 200) {",
									"    var jsonData = pm.response.json();",
									"",
									"    if (jsonData.token) {",
									"        // --- ENVIRONMENT variable ---",
									"        pm.environment.set(\"Admin-auth-token\", jsonData.token);",
									"",
									"        // --- COLLECTION variable ---",
									"        pm.collectionVariables.set(\"Admin-auth-token\", jsonData.token);",
									"",
									"        console.log(\"Admin token saved to env and collection as 'Admin-auth-token'\");",
									"    } else {",
									"        console.warn(\"No token found in admin login response\");",
									"    }",
									"}"
								],
